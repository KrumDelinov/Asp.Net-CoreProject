@model ESchool.Web.ViewModels.Classrooms.ClassroomViewModel

@{
    ViewData["Title"] = "Details";
}
<div>
    <h4>Classroom</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Име
        </dt>
        <dd class="col-sm-10">
            @Model.Number
        </dd>
        <dt class="col-sm-2">
            Фамилия
        </dt>
        <dd class="col-sm-10">
            @Model.Description
        </dd>
        <dt class="col-sm-2">
            Предмет
        </dt>
        <dd class="col-sm-10">
            @Model.TeacherFirstName
        </dd>
       

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="All">Back to List</a>
</div>

